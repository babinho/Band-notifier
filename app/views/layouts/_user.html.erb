<div id="user_nav">
  <% if current_user %>
    <%= link_to t('activerecord.models.users.profile'), edit_user_path(:current) %> |
    <%= link_to t('activerecord.models.user_sessions.logout'), logout_path %>
  <% else %>
    <%= link_to t('activerecord.models.user_sessions.login'), login_path %>
  <% end %>
</div>